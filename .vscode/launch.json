{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "gulp ut",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["ut"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy"
            ],
            "console": "internalConsole",
            "skipFiles": [
                "<node_internals>/**"
            ],
        }
    ]
}
